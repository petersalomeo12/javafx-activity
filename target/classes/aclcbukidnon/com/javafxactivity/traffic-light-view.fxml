<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<HBox alignment="CENTER" prefHeight="411.0" prefWidth="638.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/11.0.14-internal"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="aclcbukidnon.com.javafxactivity.controllers.TrafficLightController">

   <children>
      <Circle fx:id="stopLight" fill="#575757" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
      <Circle fx:id="holdLight" fill="#575757" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
      <Circle fx:id="goLight" fill="#575757" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
   </children>

   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</HBox>